{
  "name": "railroad/doctrine",
  "type": "library",
  "description": "Create doctrine package",
  "prefer-stable": true,
  "require": {
    "php": "~7.1",
    "laravel/framework": "5.6.*",
    "doctrine/orm": "^2.6",
    "doctrine/data-fixtures": "^1.3",
    "gedmo/doctrine-extensions": "^2.4",
    "jms/serializer": "1.*",
    "spatie/laravel-fractal": "^5.4",
    "ext-redis": "*",
    "railroad/doctrine-array-hydrator": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "filp/whoops": "~2.1",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Doctrine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Doctrine\\Tests\\": "tests/"
    }
  }
}
