{
  "name": "railroad/doctrine",
  "type": "library",
  "description": "Create doctrine package",
  "prefer-stable": true,
  "require": {
    "php": "~7.1",
    "ext-redis": "*",
    "laravel/framework": "5.6.*",
    "doctrine/orm": "2.7.*",
    "doctrine/data-fixtures": "^1.3",
    "gedmo/doctrine-extensions": "^2.4",
    "spatie/laravel-fractal": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "filp/whoops": "~2.1",
    "orchestra/testbench": "~3.6"
  },
  "autoload": {
    "files": [
      "src/Helpers/entity_array_helpers.php"
    ],
    "psr-4": {
      "Railroad\\Doctrine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Doctrine\\Tests\\": "tests/"
    }
  }
}
