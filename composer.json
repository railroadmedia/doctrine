{
  "name": "railroad/doctrine",
  "type": "library",
  "description": "Create doctrine package",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "doctrine/orm": "2.12.*",
    "doctrine/data-fixtures": "^1.5",
    "gedmo/doctrine-extensions": "^3.6",
    "spatie/laravel-fractal": "^6.0",
    "ext-redis": "*",
    "symfony/cache": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "files": [
      "src/Helpers/entity_array_helpers.php"
    ],
    "psr-4": {
      "Railroad\\Doctrine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Doctrine\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}
